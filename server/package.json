{
  "name": "hoadirector",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "author": "Dexter St Pierre <dexter.stpierre.dev@gmail.com>",
  "scripts": {
    "build-dev": "npm run clean && npm run build-server",
    "build-server-watch": "tsc-watch -p tsconfig.server.json --onSuccess \"npm run copy-static-assets\"",
    "build-server": "tsc -p tsconfig.server.json",
    "build": "npm run clean && npm run build-server && npm run copy-static-assets",
    "clean": "rm -fR dist/",
    "copy-static-assets": "npx ts-node copyStaticAssets.ts",
    "dev": "npm run build-dev && npm run watch",
    "lint-server": "npx tslint -c tslint.json -p tsconfig.server.json",
    "serve-watch": "npx nodemon dist/server.js",
    "start": "node dist/server.js"
  },
  "main": "dist/server.js",
  "dependencies": {
    "@bugsnag/js": "^6.5.2",
    "@bugsnag/plugin-express": "^6.5.1",
    "bcrypt": "^3.0.8",
    "concurrently": "^3.6.1",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "nodemailer": "^4.7.0",
    "npx": "^10.2.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^4.44.4",
    "shelljs": "^0.8.3",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/bcrypt": "^2.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.0",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/mysql": "^2.15.9",
    "@types/node": "^13.9.3",
    "@types/nodemailer": "^4.6.8",
    "@types/passport": "^0.4.7",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "github:types/sequelize",
    "@types/uuid": "^3.4.8",
    "@types/validator": "^12.0.1",
    "codelyzer": "^5.2.1",
    "nodemon": "^1.19.4",
    "sequelize-typescript": "^1.1.0",
    "ts-node": "~5.0.1",
    "tsc-watch": "^1.1.39",
    "tslint": "^5.9.1",
    "typescript": "~3.5.3"
  }
}
