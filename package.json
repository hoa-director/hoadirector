{
  "name": "hoadirector",
  "version": "0.0.0",
  "scripts": {
    "build-server": "tsc -p tsconfig.server.json",
    "build-server-watch": "tsc-watch -p tsconfig.server.json --onSuccess \"npm run copy-static-assets\"",
    "build-client": "ng build --prod",
    "build-client-dev": "ng build",
    "build-client-watch": "ng build --watch",
    "build-dev": "npm run clean && npm run build-server && npm run build-client-dev",
    "build": "npm run clean && npm run build-server && npm run build-client && npm run copy-static-assets",
    "clean": "rm -fR dist/",
    "copy-static-assets": "npx ts-node copyStaticAssets.ts",
    "lint-server": "npx tslint -c tslint.json -p tsconfig.server.json",
    "lint-client": "npx tslint -c tslint.json -p src/client/tsconfig.app.json",
    "lint": "npm run lint-server && npm run lint-client",
    "serve-watch": "npx nodemon dist/server.js",
    "serve": "node dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node,Angular\" \"npm:serve-watch\" \"npm:build-server-watch\" \"npm:build-client-watch\"",
    "dev": "npm run build-dev && npm run watch",
    "start": "npm run serve",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "10.5.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "angular-custom-modal": "^1.2.0",
    "bcrypt": "^3.0.0",
    "bootstrap": "^4.1.2",
    "concurrently": "^3.6.0",
    "core-js": "^2.5.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mysql2": "^1.5.3",
    "npx": "^10.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "rxjs": "^6.0.0",
    "sequelize": "^4.38.0",
    "shelljs": "^0.8.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/bcrypt": "^2.0.0",
    "@types/bluebird": "^3.5.20",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/mysql": "^2.15.5",
    "@types/node": "~8.9.4",
    "@types/passport": "^0.4.6",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "github:types/sequelize",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.18.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsc-watch": "^1.0.26",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
